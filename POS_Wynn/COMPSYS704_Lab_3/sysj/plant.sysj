import run.GUI;

Plant(
		input signal orderStart;
		input String signal liquidMix;
		input Integer signal bottleQuantity;
		)
->
{
	new Thread(new GUI()).start();

	{// 
		while(true){
			present(orderStart){
				Integer y;
				String x; 
				
				y = #bottleQuantity;
				x = #liquidMix; 
				
				System.out.println("Bottle Quantity: " + y);
				System.out.println("Liquid Mix: " + x);
			}
			pause;
		}
	}
}